@model List<Alumno>

@{
    ViewData["Title"] = "Alumno information";
    Layout = "_Simple";
}
<h1>@ViewData["Title"]</h1>
<div class="text-left">
    <strong>Desde la vista normal</strong>
    <table class="table table-dark table-hover">
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var alumno in @Model)
            {
                <tr>
                <td>@alumno.UniqueId</td>
                <td>@alumno.Name</td>
                </tr>
            }          
        </tbody>
        </table>
    @*@foreach (var Alum in Model)
    {
        @await Html.PartialAsync("~/Views/Shared/_ObjetoSchoolSimple.cshtml", Alum)
    }*@
</div>


<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>@ViewBag.CosaDinamica</p>
    <p>@ViewData["Fecha"]</p>
</div>
