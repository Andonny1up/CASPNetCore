@model List<Asignatura>

@{
    ViewData["Title"] = "Asignatura information";
    Layout = "_Simple";
}
<h1>@ViewData["Title"]</h1>
<div class="text-left">
    <strong>Desde la vista normal</strong>
@foreach (var Asig in Model)
{
  @await Html.PartialAsync("~/Views/Shared/_AsigSimple.cshtml", Asig)
}
</div>


<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>@ViewBag.CosaDinamica</p>
    <p>@ViewData["Fecha"]</p>
</div>
